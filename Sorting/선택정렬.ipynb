   "source": [
    "array=[7,5,9,0,3,1,6,2,4,8]\n",
    "\n",
    "for i in range(len(array)):\n",
    "    #i 번째 부터 선택 시작\n",
    "    min_id=i\n",
    "    for j in range(i+1,len(array)):\n",
    "        if array[min_id]>array[j]:\n",
    "            min_id=j\n",
    "    array[i],array[min_id]=array[min_id],array[i]\n",
    "print(array)"
   ]
