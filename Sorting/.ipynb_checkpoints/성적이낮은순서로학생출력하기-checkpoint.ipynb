   "source": [
    "#아코테 정렬 연습 3\n",
    "#N 입력받기\n",
    "n= int(input())\n",
    "\n",
    "#이름과 성적 입력받기\n",
    "array=[]\n",
    "for i in range(n):\n",
    "    input_data=input().split()\n",
    "    array.append([input_data[0],int(input_data[1])])\n",
    "\n",
    "array=sorted(array,key=lambda x:x[1]) #key를 이용한 정렬(몰랐던 점!)\n",
    "\n",
    "for student in array:\n",
    "    print(student[0], end=' ')"
   ]
