    "#https://school.programmers.co.kr/learn/courses/30/lessons/12945\n",
    "def solution(n):\n",
    "    #간단한 dp 문제\n",
    "    dp=[0]*100001\n",
    "    dp[0]=0;dp[1]=1\n",
    "    for i in range(2,100001):\n",
    "        dp[i]=(dp[i-1]+dp[i-2])%1234567\n",
    "    return dp[n]"
   ]
  }
