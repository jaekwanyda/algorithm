    "#https://school.programmers.co.kr/learn/courses/30/lessons/12941\n",
    "def solution(A,B):\n",
    "    answer = 0\n",
    "    A=sorted(A,key=lambda x:x)\n",
    "    B=sorted(B,key=lambda x:-x)\n",
    "    for i in range(len(A)):\n",
    "        answer+=A[i]*B[i]\n",
    "    return answer"
   ]
  }
