   "source": [
    "#이코테 기출 8\n",
    "#아이디어: 문자열을 sort하는 방법과 데이터 타입을 구별하는 함수만 알면 구현 가능할 것같다.\n",
    "#문자열 입력받기\n",
    "array=list(input())\n",
    "sum=0\n",
    "a=[]\n",
    "for i in array:\n",
    "    try :\n",
    "        sum+=int(i)\n",
    "    except ValueError:\n",
    "        a.append(i)\n",
    "a.sort() #문자열 정렬\n",
    "a=a+[sum]\n",
    "\n",
    "for i in a:\n",
    "    print(i,end='')"
   ]
